.page-list-container
  padding-top 12px
  padding-right 12px
  padding-left 12px
  .page-list-title
    margin-bottom 4px
    padding-left 6px

.page-list-title-strong, .page-list-table-id-column
  color #1890ff

.page-list-table .ant-table-body::-webkit-scrollbar, .page-list-table .ant-table-header::-webkit-scrollbar
  display none

.page-list-table .ant-table-header
  height 62px

.page-list-pagination
  margin-top 12px
  margin-bottom 12px
  padding-right 21px
  text-align right

.page-list-pagination-tip
  float left
  margin 18px 0 0 10px
  strong
    color #1890ff
    font-weight bold
